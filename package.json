{
  "name": "@nordicsemiconductor/nrfcloud-location-services-tests",
  "version": "0.0.0-development",
  "description": "Verifies A-GPS messages sent by nRF Cloud",
  "main": "dist/src/index.js",
  "typings": "dist/src/index.d.ts",
  "scripts": {
    "test": "jest src",
    "api:tests": "jest api-verification",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NordicSemiconductor/nrfcloud-location-services-tests-js.git"
  },
  "bugs": {
    "url": "https://github.com/NordicSemiconductor/nrfcloud-location-services-tests-js/issues"
  },
  "homepage": "https://github.com/NordicSemiconductor/nrfcloud-location-services-tests-js",
  "keywords": [
    "Cloud",
    "IoT",
    "nRF Asset Tracker",
    "Nordic Semiconductor"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@commitlint/cli": "13.2.1",
    "@commitlint/config-angular": "13.2.0",
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "9.1.0",
    "@nordicsemiconductor/eslint-config-asset-tracker-cloud-typescript": "8.1.7",
    "@types/jest": "27.0.2",
    "@types/jsonwebtoken": "8.5.5",
    "@types/node": "16.10.2",
    "@types/uuid": "8.3.1",
    "eslint": "8.0.0",
    "husky": "7.0.2",
    "jest": "27.2.5",
    "lint-staged": "11.2.3",
    "prettier": "2.4.1",
    "ts-jest": "27.0.5",
    "ts-loader": "9.2.6",
    "typescript": "4.4.3",
    "uuid": "8.3.2"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --ext .js,.ts"
    ],
    "*.{md,json,yaml,yml}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=7.0.0"
  },
  "release": {
    "branches": [
      "saga",
      {
        "name": "!(saga|v[0-9].[0-9].x)",
        "prerelease": true
      }
    ],
    "remoteTags": true
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "package-lock.json",
    "dist/src",
    "LICENSE",
    "README.md"
  ],
  "jest": {
    "preset": "ts-jest",
    "testRegex": "(src|api-verification)\\/.+\\.spec\\.ts$"
  },
  "dependencies": {
    "fp-ts": "2.11.4",
    "jsonwebtoken": "8.5.1"
  }
}
