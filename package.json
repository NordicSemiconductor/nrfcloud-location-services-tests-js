{
  "name": "@nordicsemiconductor/nrfcloud-location-services-tests",
  "version": "0.0.0-development",
  "description": "Verifies the nRF Cloud Location Services REST API.",
  "type": "module",
  "main": "dist/src/index.js",
  "typings": "dist/src/index.d.ts",
  "scripts": {
    "test": "npx tsx --test ./src/*.spec.ts",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NordicSemiconductor/nrfcloud-location-services-tests-js.git"
  },
  "bugs": {
    "url": "https://github.com/NordicSemiconductor/nrfcloud-location-services-tests-js/issues"
  },
  "homepage": "https://github.com/NordicSemiconductor/nrfcloud-location-services-tests-js",
  "keywords": [
    "nrf-cloud",
    "location-services",
    "end-to-end-testing"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@bifravst/eslint-config-typescript": "6.0.8",
    "@bifravst/prettier-config": "1.0.0",
    "@commitlint/config-conventional": "18.6.2",
    "@types/jsonwebtoken": "9.0.6",
    "@types/node": "20.11.21",
    "husky": "9.0.11",
    "ts-loader": "9.5.1",
    "tsx": "4.7.1"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint"
    ],
    "*.{md,json,yaml,yml}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=20",
    "npm": ">=9"
  },
  "release": {
    "branches": [
      "saga",
      {
        "name": "!(saga|v[0-9].[0-9].x)",
        "prerelease": true
      }
    ],
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      [
        "@semantic-release/github",
        {
          "successComment": false,
          "failTitle": false
        }
      ]
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "package-lock.json",
    "dist/src",
    "LICENSE",
    "README.md"
  ],
  "dependencies": {
    "jsonwebtoken": "9.0.2"
  },
  "prettier": "@bifravst/prettier-config"
}
