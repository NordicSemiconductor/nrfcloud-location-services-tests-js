{
  "name": "@nordicsemiconductor/nrfcloud-location-services-tests",
  "version": "0.0.0-development",
  "description": "Verifies the nRF Cloud Location Services REST API.",
  "main": "dist/src/index.js",
  "typings": "dist/src/index.d.ts",
  "scripts": {
    "test": "jest src",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NordicSemiconductor/nrfcloud-location-services-tests-js.git"
  },
  "bugs": {
    "url": "https://github.com/NordicSemiconductor/nrfcloud-location-services-tests-js/issues"
  },
  "homepage": "https://github.com/NordicSemiconductor/nrfcloud-location-services-tests-js",
  "keywords": [
    "nrf-cloud",
    "location-services",
    "end-to-end-testing"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "12.0.3",
    "@swc/jest": "0.2.24",
    "@types/jest": "29.4.0",
    "@types/jsonwebtoken": "9.0.1",
    "@types/node": "18.15.1",
    "jest": "29.5.0",
    "ts-loader": "9.4.2"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --ext .js,.ts"
    ],
    "*.{md,json,yaml,yml}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=18",
    "npm": ">=9"
  },
  "release": {
    "branches": [
      "saga",
      {
        "name": "!(saga|v[0-9].[0-9].x)",
        "prerelease": true
      }
    ],
    "remoteTags": true
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "package-lock.json",
    "dist/src",
    "LICENSE",
    "README.md"
  ],
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc/jest"
      ]
    },
    "testRegex": "(src|api-verification)\\/.+\\.spec\\.ts$"
  },
  "dependencies": {
    "jsonwebtoken": "9.0.0"
  }
}
